{
  "name": "TestProjectJS",
  "version": "1.0.0",
  "description": "Test project for seeding NodeJS projects",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "codacy": "NODE_ENV=test ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "codecov": "NODE_END=test ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R spec && codecov"
  },
  "keywords": [
    "scaffolding",
    "template",
    "boilerplate"
  ],
  "author": "Justin Gross",
  "license": "GPL-3.0",
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "standard",
      "google"
    ],
    "plugins": [
      "standard"
    ],
    "env": {
      "node": true
    }
  },
  "devDependencies": {
    "atom-ui-reporter": "0.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-require": "^1.0.1",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "codecov": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nsp": "^2.8.1",
    "rewire": "^2.5.2",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0",
    "v8-profiler": "^5.7.0"
  },
  "dependencies": {
    "logrocket": "^0.5.5",
    "rollbar": "^2.2.10"
  }
}
